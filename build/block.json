{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 2,
  "name": "gutenbergp5/p5js",
  "version": "1.0.0",
  "title": "p5.js",
  "category": "formatting",
  "description": "Add custom p5.js code and preview it as you edit.",
  "supports": {
    "html": false
  },
  "textdomain": "gutenbergp5",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css",
  "attributes": {
    "sketch": {
      "type": "string",
      "default": "function setup() {\n\tcreateCanvas(600, 600);\n}\n\nfunction draw() {\n\t// Your code here\n}"
    },
    "alignment": {
      "type": "string",
      "default": "center"
    },
    "hasScrollbar": {
      "type": "boolean",
      "default": false
    },
    "width": {
      "type": "string",
      "default": ""
    },
    "height": {
      "type": "string",
      "default": ""
    },
    "isPreview": {
      "type": "boolean",
      "default": false
    }
  }
}